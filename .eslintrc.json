{
  "extends": "airbnb",
  "env": {
    "mocha": true,
    "node": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "global-require": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "key-spacing": 0,
    "import/no-extraneous-dependencies": 0,
    "padded-blocks": 0,
    "no-unused-expressions": 0,
    "import/newline-after-import": 0,
    "no-console" : 0
  }
}